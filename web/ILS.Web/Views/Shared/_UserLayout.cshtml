 <script type="text/javascript">
     var sdoorl = true;
     var sdoorr = true;

     var sdoorlTimer;
     var sdoorlLeft = 0;
     var sdoorrTimer;
     var sdoorrLeft = 0;

     function sdoor_open(dir) {
         if (dir == 'l') {
             clearInterval(sdoorlTimer);
             function move() {
                 sdoorlLeft -= 3;
                 document.getElementById('sdoorll').style.left = sdoorlLeft + 'px';
                 document.getElementById('sdoorlr').style.left = 46 - sdoorlLeft + 'px';
                 if (sdoorlLeft <= -102) {
                     clearInterval(sdoorlTimer);
                 }
             }
             sdoorlTimer = setInterval(move, 1)
         } else {
             clearInterval(sdoorrTimer);
             function move() {
                 sdoorrLeft -= 3;
                 document.getElementById('sdoorrl').style.left = sdoorrLeft + 'px';
                 document.getElementById('sdoorrr').style.left = 46 - sdoorrLeft + 'px';
                 if (sdoorrLeft <= -102) {
                     clearInterval(sdoorrTimer);
                 }
             }
             sdoorrTimer = setInterval(move, 1);
         }
     }

     function sdoor_close(dir) {
         if (dir == 'l') {
             clearInterval(sdoorlTimer);
             function move() {
                 sdoorlLeft += 3;
                 document.getElementById('sdoorll').style.left = sdoorlLeft + 'px';
                 document.getElementById('sdoorlr').style.left = 46 - sdoorlLeft + 'px';
                 if (sdoorlLeft >= 0) {
                     clearInterval(sdoorlTimer);
                 }
             }
             sdoorlTimer = setInterval(move, 1);
         } else {
             clearInterval(sdoorrTimer);
             function move() {
                 sdoorrLeft += 3;
                 document.getElementById('sdoorrl').style.left = sdoorrLeft + 'px';
                 document.getElementById('sdoorrr').style.left = 46 - sdoorrLeft + 'px';
                 if (sdoorrLeft >= 0) {
                     clearInterval(sdoorrTimer);
                 }
             }
             sdoorrTimer = setInterval(move, 1);
         }
     }

     function sdoor_force_l() {
         sdoor_close('l');
     }
     function sdoor_force_r() {
         sdoor_close('r');
     }

     var framel = 0;

     var demolTimer;

     function demol_animate() {
         clearInterval(demolTimer);
         var frames = document.getElementById("demolcontainer").children;
         function move() {
             frames[framel].style.display = "none";
             framel++;
             if (framel == 12) {
                 framel = 6;
             }
             console.log(framel);
             frames[framel].style.display = "block";

         }
         demolTimer = setInterval(move, 100);
     }

     function demol_stop() {
         clearInterval(demolTimer);
         var frames = document.getElementById("demolcontainer").children;
         function move() {
             if (framel < 1) {
                 clearInterval(demolTimer);
                 framel = 1;
             }
             frames[framel].style.display = "none";
             framel--;
             console.log(framel);
             frames[framel].style.display = "block";

         }
         demolTimer = setInterval(move, 100);
     }


     var framer = 0;

     var demorTimer;

     function demor_animate() {
         clearInterval(demorTimer);
         var frames = document.getElementById("demorcontainer").children;
         function move() {
             frames[framer].style.display = "none";
             framer++;
             if (framer == 12) {
                 framer = 6;
             }
             console.log(framer);
             frames[framer].style.display = "block";

         }
         demorTimer = setInterval(move, 70);
     }

     function demor_stop() {
         clearInterval(demorTimer);
         var frames = document.getElementById("demorcontainer").children;
         function move() {
             if (framer < 1) {
                 clearInterval(demorTimer);
                 framer = 1;
             }
             frames[framer].style.display = "none";
             framer--;
             console.log(framer);
             frames[framer].style.display = "block";

         }
         demorTimer = setInterval(move, 70);
     }


     var framep = 0;

     var playTimer;

     function play_animate() {
         clearInterval(playTimer);
         var frames = document.getElementById("playcontainer").children;
         function move() {
             frames[framep].style.display = "none";
             framep++;
             if (framep == 24) {
                 framep = 12;
             }
             console.log(framep);
             frames[framep].style.display = "block";

         }
         playTimer = setInterval(move, 70);
     }

     function play_stop() {
         clearInterval(playTimer);
         var frames = document.getElementById("playcontainer").children;
         frames[framep].style.display = "none";
         framep = 0;
         console.log("t"+framep);
         frames[0].style.display = "block";
         
     }
</script>

  
    
    @RenderSection("scripts", required:false)
